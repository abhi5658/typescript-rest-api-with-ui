  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run dev server",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}\\server",
      "env": {
        "MONGODB_URI": "mongodb+srv://username:password@cloud-localhost-xxx-xxx.xxxxx.mongodb.net/ts_restapi_with_ui",
      },
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
  ]
}